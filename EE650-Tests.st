TestCase subclass: #StoryS1Test	instanceVariableNames: ''	classVariableNames: ''	package: 'EE650-Tests'!!StoryS1Test methodsFor: 'tests' stamp: '4/12/2025 16:49'!testUserManagerAddUser    | userManager |    userManager := UserManager new.    userManager addUser: 'testuser' password: 'password123'.    self assert: (userManager userExists: 'testuser').! !!StoryS1Test methodsFor: 'tests' stamp: '4/12/2025 16:50'!testAuthServiceInitialization    | authService |    authService := AuthService new.    self assert: authService userManager isNotNil.! !!StoryS1Test methodsFor: 'tests' stamp: '4/12/2025 16:52'!testUserCreation    | user |    user := User new username: 'testuser'; password: 'password123'.    self assert: user username equals: 'testuser'.    self assert: user password equals: 'password123'.! !TestCase subclass: #StoryS2Test	instanceVariableNames: ''	classVariableNames: ''	package: 'EE650-Tests'!!StoryS2Test methodsFor: 'tests' stamp: '4/12/2025 17:03'!testUserRegistration    | authService userManager |    userManager := UserManager new.    authService := AuthService new userManager: userManager.    authService registerUser: 'testuser' password: 'password123'.    self assert: (userManager userExists: 'testuser').! !!StoryS2Test methodsFor: 'tests' stamp: '4/12/2025 17:03'!testSuccessfulLogin    | authService userManager |    userManager := UserManager new.    authService := AuthService new userManager: userManager.    authService registerUser: 'testuser' password: 'password123'.    self assert: (authService loginWithUsername: 'testuser' password: 'password123').! !!StoryS2Test methodsFor: 'tests' stamp: '4/12/2025 17:04'!testFailedLogin    | authService userManager |    userManager := UserManager new.    authService := AuthService new userManager: userManager.    authService registerUser: 'testuser' password: 'password123'.    self assert: (authService loginWithUsername: 'testuser' password: 'wrongpassword') not.! !